{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "serve public --listen ${PORT:-8080}",
    "start": "npm run dev & wait-on http://127.0.0.1:8080",
    "test": "cypress run --reporter mochawesome --reporter-options 'reportDir=cypress/report/mochawesome-report,overwrite=false,html=false,json=true,timestamp=mmddyyyy_HHMMss'",
    "test:report:merge": "mochawesome-merge cypress/report/**/*.json -o cypress/report/mochawesome.json"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "14.0.13",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "cypress": "4.8.0",
    "mocha": "8.0.1",
    "mochawesome": "6.1.1",
    "mochawesome-merge": "4.1.0",
    "serve": "11.3.2",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "wait-on": "5.0.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
