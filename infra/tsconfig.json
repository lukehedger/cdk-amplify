{
  "compilerOptions": {
    "declaration": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "ES2018",
    "types": ["jest", "node"],
    "typeRoots": ["../node_modules/@types"]
  },
  "exclude": ["cdk.out"]
}
